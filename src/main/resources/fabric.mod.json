{
  "schemaVersion": 1,
  "id": "multiblocktab",
  "version": "${version}",

  "name": "Multiblock Tab",
  "description": "Allows mod creators to display their multiblocks including additional descriptions using an AMI tab.",
  "authors": [
    "Ralf2oo2, Atilist"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/stationapi-example-mod"
  },

  "license": "MIT",
  "icon": "assets/multiblocktab/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.newfrontiercraft.multiblocktab.MultiblockTab"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ],
    "alwaysmoreitems:plugin": [
      "net.newfrontiercraft.multiblocktab.MultiblockPluginProvider"
    ]
  },
  "mixins": [
    "multiblocktab.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
